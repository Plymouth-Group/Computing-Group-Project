SRC := cmd/common.go    \
			 cmd/dashboard.go \
			 cmd/global.go    \
			 cmd/database.go  \
			 cmd/domain.go    \
			 cmd/editor.go    \
			 cmd/email.go     \
			 cmd/forgot.go    \
			 cmd/main.go      \
			 cmd/signin.go    \
			 cmd/signup.go

RUN:
	go run $(SRC)

BUILD:
	go build $(SRC)
	./codinoc